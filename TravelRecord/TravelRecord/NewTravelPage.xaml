        <ToolbarItem Clicked="ToolbarItem_Clicked" Text="Save"></ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Write your experience" x:Name="xpEntry"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>